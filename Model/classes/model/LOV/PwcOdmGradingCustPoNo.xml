<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PwcOdmGradingCustPoNo"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false"
  FetchMode="FETCH_AS_NEEDED"
  FetchSize="20"
  RangeSize="21">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.PwcOdmGradingCustPoNo_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_org_id"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="p_order_no"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery>
    <![CDATA[select distinct  oola.CUST_PO_NUMBER
,ooha.ORDER_NUMBER
,party.party_name
--,ood.ORGANIZATION_ID
from oe_order_headers_all ooha, 
oe_order_lines_all oola
,hz_cust_accounts cust_acct
,hz_parties party
,org_organization_definitions ood
where  ooha.HEADER_ID = oola.HEADER_ID
and ooha.ORG_ID = oola.ORG_ID
and ooha.ORG_ID = ood.OPERATING_UNIT
and ooha.SOLD_TO_ORG_ID = cust_acct.cust_account_id(+)
AND cust_acct.party_id = party.party_id(+)
and ooha.ORDER_NUMBER = nvl(:p_order_no,ooha.ORDER_NUMBER)
and ood.ORGANIZATION_ID = nvl(:p_org_id,ood.ORGANIZATION_ID)
and oola.cust_po_number is not null]]>
  </SQLQuery>
  <ViewAttribute
    Name="CustPoNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CUST_PO_NUMBER"
    Expression="CUST_PO_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
        <LABEL
          ResId="model.LOV.PwcOdmGradingCustPoNo.CustPoNumber_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="OrderNumber"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ORDER_NUMBER"
    Expression="ORDER_NUMBER"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="PartyName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="360"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARTY_NAME"
    Expression="PARTY_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="360"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="CustPoNumber"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
