  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.PwcOdmGradingSalesOrderNoVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_org_id"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="p_cust_po_no"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery>
    <![CDATA[select distinct ooha.ORDER_NUMBER, party.party_name 
 from oe_order_headers_all ooha,oe_order_lines_all oola, org_organization_definitions ood , hz_parties party, hz_cust_accounts cust_acct
 where ooha.header_id = oola.header_id
 and ooha.ORG_ID = ood.OPERATING_UNIT
 AND ooha.sold_to_org_id = cust_acct.cust_account_id(+)
 AND cust_acct.party_id = party.party_id(+)
 and oola.CUST_PO_NUMBER = nvl(:p_cust_po_no,oola.CUST_PO_NUMBER)
 and ood.ORGANIZATION_ID = nvl(:p_org_id,ood.ORGANIZATION_ID)]]>
  </SQLQuery>
  <ViewAttribute
